{
  "name": "pg-viewer",
  "version": "0.0.4-beta",
  "description": "A PostgreSQL database viewer with a web interface you can run locally",
  "main": "cli.mjs",
  "bin": "./cli.mjs",
  "keywords": [
    "postgresql",
    "database",
    "viewer",
    "cli",
    "web",
    "interface"
  ],
  "author": "Elora",
  "license": "MIT",
  "homepage": "https://pg.aruu.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/eloraa/pg-viewer"
  },
  "files": [
    "cli.mjs",
    ".next/**/*",
    "https/certs/**/*",
    "next.config.ts",
    "README.md",
